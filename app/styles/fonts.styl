// regular
@font-face {
	font-family: "Lato"
	font-style: normal
	font-weight: 400
	src: local("Lato Regular"),
		local("Lato-Regular"),
		url("../fonts/Lato-Regular.ttf") format("truetype")
}

// bold
@font-face {
	font-family: "Lato"
	font-style: normal
	font-weight: 700
	src: local("Lato Bold"),
		local("Lato-Bold"),
		url("../fonts/Lato-Bold.ttf") format("truetype")
}
